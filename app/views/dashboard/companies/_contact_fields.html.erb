<div class="nested-fields">
  <%= f.hidden_field :_destroy %>
  <div class="form-group">
    <%= f.label :phone %>
    <%= f.text_field :phone, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :message %>
    <%= f.text_field :message, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :commercial %>
    <%= f.check_box :commercial, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :financial %>
    <%= f.check_box :financial, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :logistics %>
    <%= f.check_box :logistics, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :administrative %>
    <%= f.check_box :administrative, class: "form-control" %>
  </div>
  <div>
    <%= link_to "Remove", '#', class: "remove_fields" %>
  </div>
</div>
